{
    "name": "elcodi/currency-bundle",
    "description": "Elcodi Currency component",
    "keywords": ["elcodi", "currency", "ecommerce"],
    "homepage": "https://github.com/elcodi/CurrencyBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Gonzalo Miguez",
            "email": "mrzard@gmail.com"
        },
        {
            "name": "Marc Morera",
            "email": "yuhu@mmoreram.com"
        },
        {
            "name": "Aldo Chiecchia",
            "email": "zimage@tiscali.it"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://github.com/elcodi/CurrencyBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/orm": "~2.4",
        "symfony/config": "~2.4",
        "symfony/form": "~2.4",
        "symfony/options-resolver": "~2.4",
        "symfony/routing": "~2.4",
        "symfony/http-foundation": "~2.4",
        "symfony/framework-bundle": "~2.4, >=2.4.8",
        "symfony/dependency-injection": "~2.4",
        "symfony/http-kernel": "~2.4",
        "twig/twig": "~1",
        "sebastian/money": "~1.3",

        "elcodi/core-bundle": "~0.2.0",
        "elcodi/language-bundle": "~0.2.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "symfony/browser-kit": "~2.4",
        "symfony/framework-bundle": "~2.4, >=2.4.8",
        "symfony/validator": "~2.4",
        "symfony/finder": "~2.4",
        "phpunit/phpunit": ">=4.0",
        "symfony/routing": "~2.4",
        "symfony/console": "~2.4"
    },
    "suggest": {
        "mrzard/open-exchange-rates-bundle": "Bundle to use OpenExchangeRates"
    },
    "autoload": {
        "psr-4": {
            "Elcodi\\CurrencyBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
