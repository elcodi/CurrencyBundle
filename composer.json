{
    "name": "elcodi/currency-bundle",
    "description": "Elcodi Currency component",
    "keywords": ["elcodi", "currency", "ecommerce"],
    "homepage": "https://github.com/elcodi/CurrencyBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Gonzalo Miguez",
            "email": "mrzard@gmail.com"
        },
        {
            "name": "Marc Morera",
            "email": "yuhu@mmoreram.com"
        },
        {
            "name": "Aldo Chiecchia",
            "email": "zimage@tiscali.it"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://github.com/elcodi/CurrencyBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/orm": "~2.4",
        "symfony/config": "~2.4",
        "symfony/options-resolver": "~2.4",
        "symfony/routing": "~2.4",
        "symfony/http-foundation": "~2.4",
        "symfony/framework-bundle": "~2.4, >=2.4.8",
        "symfony/dependency-injection": "~2.4",
        "symfony/http-kernel": "~2.4",
        "sebastian/money": "~1.3",

        "elcodi/core-bundle": "~0.2.0"
    },
    "require-dev": {
        "elcodi/test-common-bundle": "~0.2.0",
        "doctrine/data-fixtures": "~1.0",
        "phpunit/phpunit": ">=3.7"
    },
    "suggest": {
        "symfony/twig-bundle": "Required if using Twig extensions",
        "elcodi/language-bundle": "Use Elcodi locale provider",
        "guzzle/http": "Required for OpenExchangeRates adapter"
    },
    "autoload": {
        "psr-4": {
            "Elcodi\\CurrencyBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
